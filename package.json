    "inquirer": "5.1.0",
    "interpret": "1.1.0",
    "is-git-url": "1.0.0",
    "liftoff": "2.5.0",
    "minimist": "1.2.0",
    "resolve-global": "0.1.0",
    "which": "1.3.0"
